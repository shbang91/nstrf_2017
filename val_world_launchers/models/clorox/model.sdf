<?xml version="1.0" ?>
<sdf version="1.5">
  <model name="clorox">
    <!-- Give the base link a unique name -->
    <link name="base">
      <pose>0 0 0 0 0 0</pose>
      <collision name="base_collision">
        <geometry>
          <mesh>
            <uri>model://clorox/meshes/clorox.dae</uri>
          </mesh>
        </geometry>
      </collision>

      <!-- The visual is mostly a copy of the collision -->
      <visual name="base_visual">
        <geometry>
           <mesh>
             <uri>model://clorox/meshes/clorox.dae</uri>
           </mesh>
        </geometry>
      </visual>
    </link>
  </model>
</sdf>
